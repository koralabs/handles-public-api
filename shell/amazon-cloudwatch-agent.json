{
	"agent": {
		"metrics_collection_interval": 60,
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/home/ubuntu/.forever/**.log",
						"log_group_name": "/koralabs/cardano-node/{{NETWORK}}",
						"log_stream_name": "{instance_id}"
					}
				]
			}
		}
	}
}